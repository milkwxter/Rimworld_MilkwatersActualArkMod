<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="AnimalThingBase">
    <defName>m_Parasaurolophus</defName>
    <label>parasaurolophus</label>
    <devNote>parasaurolophus</devNote>
    <description>The parasaurolophus, or parasaur, is a medium sized, herbivorous dinosaur known for its ability to honk and scare away predators. It can also eminate a wave from its crest to detect nearby the prescence of nearby hostiles.</description>
	
	<comps>
		<li Class="Milkwaters_ArkMod.CompProperties_ColorRegions" />
		<li Class="CompProperties_EggLayer">
			<eggUnfertilizedDef>m_Egg_ParasaurUnfertilized</eggUnfertilizedDef>
			<eggFertilizedDef>m_Egg_ParasaurFertilized</eggFertilizedDef>
			<eggFertilizationCountMax>1</eggFertilizationCountMax>
			<eggLayIntervalDays>8</eggLayIntervalDays>
			<eggCountRange>1</eggCountRange>
		</li>
	</comps>
	<modExtensions>
		<!--
		<li MayRequire="Owlchemist.GiddyUp" Class="GiddyUp.Mountable" />
		<li MayRequire="Owlchemist.GiddyUp" Class="GiddyUp.DrawInFront" />
		-->
	</modExtensions>
	
    <statBases>
      <MoveSpeed>5.3</MoveSpeed>
      <MarketValue>500</MarketValue>
      <ComfyTemperatureMin>-10</ComfyTemperatureMin>
      <FilthRate>6</FilthRate>
      <Wildness>0.0</Wildness>
      <ArmorRating_Sharp>0.15</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.15</ArmorRating_Blunt>
      <ArmorRating_Heat>0.10</ArmorRating_Heat>
    </statBases>
	
    <race>
	  <herdAnimal>true</herdAnimal>
      <predator>false</predator>
      <body>m_Theropod</body>
      <leatherDef>Leather_Lizard</leatherDef>
      <baseHungerRate>0.4</baseHungerRate>
      <baseBodySize>2.5</baseBodySize>
      <baseHealthScale>2.0</baseHealthScale>
      <foodType>VegetarianRoughAnimal</foodType>
      <trainability>Intermediate</trainability>
      <roamMtbDays>7</roamMtbDays>
      <mateMtbHours>8</mateMtbHours>
      <lifeExpectancy>33</lifeExpectancy>
	  
      <manhunterOnTameFailChance>0.00</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.00</manhunterOnDamageChance>
	  
		<wildBiomes>
			<TemperateForest>0.28</TemperateForest>
			<TemperateSwamp>0.20</TemperateSwamp>
			<TropicalRainforest>0.12</TropicalRainforest>
			<AridShrubland>0.06</AridShrubland>
		</wildBiomes>
	  
      <packAnimal>true</packAnimal>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.20</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.33</minAge>
          <soundWounded>m_Parasaur_Wounded</soundWounded>
          <soundDeath>m_Parasaur_Death</soundDeath>
          <soundCall>m_Parasaur_Call</soundCall>
          <soundAngry>m_Parasaur_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
	
    <tradeTags>
      <li>AnimalUncommon</li>
    </tradeTags>
	
    <tools>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>11</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>1.2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
  </ThingDef>

